{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [true, "tabs", 2],
    "member-access": [true, "no-public"],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "arrays": "always",
          "functions": "never",
          "objects": "always"
        }
      }
    ],
    "quotemark": [true, "single"],
    "no-console": false
  },
  "rulesDirectory": []
}